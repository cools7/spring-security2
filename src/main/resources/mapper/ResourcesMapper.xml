<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iflytek.springsecurity.mapper.ResourcesMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.iflytek.springsecurity.entity.Resources">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="res_url" property="resUrl"/>
        <result column="type" property="type"/>
        <result column="parent_id" property="parentId"/>
        <result column="sort" property="sort"/>
        <result column="res_key" property="resKey"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
            id, name, res_url, type, parent_id, sort, res_key
        </sql>


</mapper>